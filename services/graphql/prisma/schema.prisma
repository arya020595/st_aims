generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ActivityLogs {
  id        BigInt    @id @default(autoincrement())
  uuid      String    @unique @db.VarChar(50)
  type      String?   @map("type")
  tableName String?   @map("tableName")
  log       Json?     @map("log")
  userId    String?   @map("user_id") @db.Text
  createdAt DateTime? @map("created_at") @db.Timestamp
  updatedAt DateTime? @map("updated_at") @db.Timestamp

  @@map("activityLogs")
}

model LivestockCategory {
  id           BigInt   @id @default(autoincrement())
  uuid         String   @unique @db.VarChar(50)
  code         String   @map("code") @db.Text
  categoryName String   @map("livestock_category") @db.Text
  createdAt    DateTime @map("created_at") @db.Timestamp
  updatedAt    DateTime @map("updated_at") @db.Timestamp
  deletedAt    String?  @map("deleted_at") @db.Text
  createdBy    Json?    @map("created_by")
  updatedBy    Json?    @map("updated_by")
  deletedBy    Json?    @map("deleted_by")
  // Livestock    Livestock[]

  @@map("livestockCategories")
}

model Livestock {
  id                  BigInt  @id @default(autoincrement())
  uuid                String  @unique @db.VarChar(50)
  // livestockCategory   LivestockCategory? @relation(fields: [livestockCategoryId], references: [uuid])
  livestockCategoryId String? @map("livestock_category_uuid") @db.VarChar(50)

  typeOfLiveStock String   @map("type_of_livestock") @db.Text
  createdAt       DateTime @map("created_at") @db.Timestamp
  updatedAt       DateTime @map("updated_at") @db.Timestamp
  deletedAt       String?  @map("deleted_at") @db.Text
  createdBy       Json?    @map("created_by")
  updatedBy       Json?    @map("updated_by")
  deletedBy       Json?    @map("deleted_by")

  // AnimalFeed AnimalFeed[]
  // AnimalFeed    AnimalFeed[]
  // LivestockFeed LivestockFeed[]

  @@map("livestocks")
}

model Supplier {
  id           BigInt   @id @default(autoincrement())
  uuid         String   @unique @db.VarChar(50)
  code         String   @map("code") @db.Text
  supplierName String   @map("supplier_name") @db.Text
  createdAt    DateTime @map("created_at") @db.Timestamp
  updatedAt    DateTime @map("updated_at") @db.Timestamp
  deletedAt    String?  @map("deleted_at") @db.Text
  createdBy    Json?    @map("created_by")
  updatedBy    Json?    @map("updated_by")
  deletedBy    Json?    @map("deleted_by")
  // AnimalFeed    AnimalFeed[]
  // LivestockFeed LivestockFeed[]

  @@map("suppliers")
}

model AnimalFeed {
  id   BigInt @id @default(autoincrement())
  uuid String @unique @db.VarChar(50)

  // liveStock   Livestock @relation(fields: [livestockId], references: [uuid])
  livestockId String? @map("type_of_livestock") @db.VarChar(50)

  // supplier    Supplier @relation(fields: [supplierId], references: [uuid])
  supplierId String? @db.VarChar(50)

  category    String @map("feed_category") @db.Text
  code        String @map("feed_code_type") @db.VarChar(50)
  description String @db.Text

  createdAt DateTime @map("created_at") @db.Timestamp
  updatedAt DateTime @map("updated_at") @db.Timestamp
  deletedAt String?  @map("deleted_at") @db.Text
  createdBy Json?    @map("created_by")
  updatedBy Json?    @map("updated_by")
  deletedBy Json?    @map("deleted_by")
  // LivestockFeed LivestockFeed[]

  @@map("animalFeeds")
}

